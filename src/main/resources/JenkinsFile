pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/ViswanthKumarKS/unit-testing.git'
            }
        }
        stage('Build') {
            steps {
                echo 'javac HelloWorld.java'
            }
        }
        stage('Test') {
            steps {
                echo 'java HelloWorld'
            }
        }
    }
}
