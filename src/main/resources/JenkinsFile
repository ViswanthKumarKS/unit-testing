pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/ViswanthKumarKS/unit-testing.git'
            }
        }
        stage('Build') {
            steps {
               echo 'javac HelloWorldControllerTest.java'
            }
        }
        stage('Test') {
            steps {
                echo 'javac HelloWorldControllerTest.java'
                echo 'java HelloWorldController' 

            }
        }
    }
}
